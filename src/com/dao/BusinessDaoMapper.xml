<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dao.BusinessDao" >

    <insert id="add" >

        insert into business (bnum,btype,bmark)values( #{bnum},#{btype},#{bmark});


    </insert>


    <insert id="increase">



        insert  into detail(bdid,bid,bdname,bdperson)values (#{bdid},#{bid},#{bdname},#{bdperson})
    </insert>

</mapper>
